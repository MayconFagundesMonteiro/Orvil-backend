// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Livros{
  id Int  @id @default(autoincrement())
  capa String
  titulo String
  autor String
  editora String?
  ano DateTime
  edicacao Int?
  genero String
}